{
  "name": "scanquestbackend",
  "version": "1.0.0",
  "description": "App para criação de simulados a partir de PDF's com questões utilizando IA",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/http/server.ts",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "prod": "node dist/shared/http/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MuriloCode7/ScanQuestBackend.git"
  },
  "keywords": [
    "Scanner",
    "PDF",
    "IA",
    "simulado"
  ],
  "author": "MuriloCode7 e victorinoli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MuriloCode7/ScanQuestBackend/issues"
  },
  "homepage": "https://github.com/MuriloCode7/ScanQuestBackend#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/eslint": "^8.56.3",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/node": "^20.11.20",
    "@types/prettier": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "rate-limiter-flexible": "^5.0.0",
    "typeorm": "^0.3.20",
    "typeorm-pagination": "^2.0.3"
  }
}
